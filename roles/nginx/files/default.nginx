## Nginx default server config

server {
	listen 80 default_server;
	listen [::]:80 default_server;
	listen 443 ssl http2 default_server;
	listen [::]:443 ssl http2 default_server;

        location /nginx_status {
                stub_status on;
                access_log off;
                allow 127.0.0.1;
		allow 10.100.0.0/16;
                deny all;
        }

	return 444;
}
